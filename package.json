{
  "name": "vue-table-view-next",
  "version": "0.0.1",
  "files": [
    "dist",
    "types",
    "README.md",
    "package.json"
  ],
  "main": "./dist/vue-table-view-next.umd.js",
  "module": "./dist/vue-table-view-next.es.js",
  "types": "./types",
  "exports": {
    ".": {
      "import": "./dist/vue-table-view-next.es.js",
      "require": "./dist/vue-table-view-next.umd.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:files",
    "build:files": "vite build",
    "build:types": "rm -rf types && vue-tsc -d --emitDeclarationOnly --declarationDir ./types",
    "build-watch": "pnpm run build:types-watch & pnpm run build:files-watch",
    "build:files-watch": "vite build --watch",
    "build:types-watch": "rm -rf types && vue-tsc -d --emitDeclarationOnly --declarationDir ./types --watch"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "element-plus": "^2.1.9",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.32",
    "vxe-table": "^4.2.2",
    "xe-utils": "^3.5.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/shared": "^3.2.31",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-no-explicit-type-exports": "^0.12.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^8.6.0",
    "prettier": "^2.6.1",
    "sass": "^1.49.10",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.8",
    "unplugin-element-plus": "^0.3.3",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vite-plugin-style-import": "^1.4.1",
    "vue-tsc": "^0.34.5"
  }
}
